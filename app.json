{
  "expo": {
    "name": "WannaWatch",
    "slug": "WannaWatch",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "scheme": "wannawatch",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lsettle.WannaWatch",
      "buildNumber": "1.0.13"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "extra": {
      "API_URL": "https://629d-74-133-228-129.ngrok-free.app",
      "WEBSOCKET_URL": "wss://wannawatch.azurewebsites.net/",
      "eas": {
        "projectId": "27986aac-b8b0-4416-ba18-02704883e73f"
      }
    },
    "description": "WannaWatch helps you and your significant other choose what to watch on date night! ",
    "plugins": [
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": ["./app/assets/fonts/Signika.ttf"]
        }
      ]
    ]
  }
}